<h1>Private Events</h1>
<ol>
  <% @events.each do | event | %>
    <li>
      <%= event.title %>
      <strong> Creator: </strong><%= link_to event.creator.username.capitalize, user_path(event.creator.id) %>(<%= event.creator.id %>)
      -> <small><%= link_to "See event", event_path(event) %></small>
    </li>
  <% end %>
</ol>
